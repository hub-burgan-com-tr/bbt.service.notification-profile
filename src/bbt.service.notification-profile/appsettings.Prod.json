{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost; Database=Reminder; User Id=XXXX;Password=YYYY; MultipleActiveResultSets=true;TrustServerCertificate=True",
    "ReminderConnectionString": "Server=server; User=ProdUser; password=ProdReminderPassword; database=db;TrustServerCertificate=True",
    "RedisConnection": "redistest"

  },
  "CustomerProfileEndpoint": "XXX",

  "MessagingGateway": {
    "EndPoints": {
      "TemplateSmsOn": "TemplateSmsUrlOn",
      "TemplateSmsBurgan": "TemplateSmsUrlBurgan",
      "TemplateEmailOn": "TemplateEmailUrlOn",
      "TemplateEmailBurgan": "TemplateEmailUrlBurgan",
      "TemplatePushOn": "TemplatePushUrlOn",
      "TemplatePushBurgan": "TemplatePushUrlBurgan"
    }
  },
  "TfsEndpoints": {

    "PutVariables": "PutTfsVariableProd",
    "CreateRelease": "CreateTfsReleaseProd"
  },
  "TfsToken": "TokenTfsProd",
  "RedisTimeout": 5,
  "RedisProductListTimeOut": 1440,
  "MetricsOptions": {
    "DefaultContextLabel": "Bbt_notification_profile_services",
    "Enabled": true
  },
  "DengageApi": {
    "Url": "DengageUrl",
    "ApiKey": {
      "Digital": {
        "UserKey": "DengageDigitalUserKey",
        "Password": "DengageDigitalPassword"
      },
      "NoneDigital": {
        "UserKey": "DengageNonDigitalUserKey",
        "Password": "DengageNonDigitalPassword"
      }
    }
  },
  "MetricsWebTrackingOptions": {
    "ApdexTrackingEnabled": true,
    "ApdexTSeconds": 0.1,
    "IgnoredHttpStatusCodes": [],
    "IgnoredRoutesRegexPatterns": [],
    "OAuth2TrackingEnabled": true
  },
  "MetricEndpointsOptions": {
    "MetricsEndpointEnabled": true,
    "MetricsTextEndpointEnabled": true,
    "EnvironmentInfoEndpointEnabled": true
  },
  "ElasticApm": {
    "SecretToken": "secretToken",
    "ServerUrl": "serverUrl",
    "ServiceName": "serviceName",
    "Environment": "Production",
    "TransactionSampleRate": 1.0,
    "CaptureBody": "all",
    "CaptureBodyContentTypes": "application/x-www-form-urlencoded*, text/*, application/json*, application/xml*"
  }
}